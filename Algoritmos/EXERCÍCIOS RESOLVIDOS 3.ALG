algoritmo "EXERCÍCIOS RESOLVIDOS 3"
          //EXERCÍCIOS RESOLVIDOS 3 DA AULA 10
var
   N, C,NN:inteiro
inicio
      C <- 1
      Repita
            escreva("Informe um número: ")
            leia(N)
            se ( N < 0) entao
               NN <- NN + 1
            fimse
            C <- C + 1
      Ate (C > 5)
      escreval(NN, " números são negativos")

fimalgoritmo