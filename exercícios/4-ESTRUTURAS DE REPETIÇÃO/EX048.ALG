algoritmo "EX048"
var
   cont, S, N:inteiro
inicio
      cont <- 1
      S <- 0
      enquanto (cont <= 7) faca
               cont <- cont + 1
               escreva("Digite um número: ")
               leia(N)
               S <- S + N

      fimenquanto
      escreval("A soma entre eles é ",S)

fimalgoritmo