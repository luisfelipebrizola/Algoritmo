algoritmo "ex090"
var
A,B,X,Y:inteiro

Procedimento Somador(A,B:inteiro)
inicio
      escreval("Recebi o valor: ",A)
      escreval("Recebi o valor: ",B)
      escreval("A soma entre eles é ",A+B)
fimprocedimento
inicio
      escreva("Informe um número: ")
      leia(X)
      escreva("Informe um segundo número: ")
      leia(Y)
      somador(X,Y)

fimalgoritmo