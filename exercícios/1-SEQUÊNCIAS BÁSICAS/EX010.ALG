   algoritmo "EX010"
   var
   AL, LA, AR, LT: real
   inicio
   escreva("Informe a altura da parede: ")
   leia(AL)
   escreva("Informe a largura da parede: ")
   leia(LA)
   AR <- AL*LA
   LT <- AR/2
   escreva("A área a ser pintada parede é de " , AR , " e serão usados " , LT , " litros de tinta.")

   fimalgoritmo